{
  "name": "vue-lazyload-text-dev",
  "version": "0.0.4-beats",
  "author": "Boyle Gu<gubaoer@hotmail.com>",
  "description": "The high performance magic text with lazyload support.",
  "keywords": [
    "lazyload",
    "vuejs",
    "vue",
    "text"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boylegu/vue-lazyload-text"
  },
  "main": "dist/vue-lazyload-text.ssr.js",
  "module": "dist/vue-lazyload-text.esm.js",
  "unpkg": "dist/vue-lazyload-text.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/lib-dev.vue"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js --progress",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:demo": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js --progress",
    "gh": "npm run build:demo && node build/deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.7.0",
    "minimist": "^1.2.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.0",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vue-lazyload-text-dev": "0.0.3-beats",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  }
}
